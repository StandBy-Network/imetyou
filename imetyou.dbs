CREATE VIEW bump.administrators_view AS 
select 
v.id, 
 to_char(v.user_registration_date, 'YYYY.MM.DD HH24:MI:SS'::text) as user_registration_date, 
v.user_login,
v.user_email, 
v.user_nickname,
uc.name_i18n as user_country_code,
v.user_city,
ut.md_code_i18n as user_type, 
null as unapproved_media
from bump.bump_user v 
left outer join bump.bump_md_collection ut on ut.id = v.user_type_mdci
left outer join bump.bump_md_country uc on uc.id = v.user_country_coi

where  v.user_role in ('SUPER_ADMIN','OPERATOR','LICENSE_PARTNER');

COMMENT ON VIEW bump.administrators_view IS 'Administratorok listához az admin felületen';

CREATE VIEW bump.deleted_users_view AS 
select 
v.id, 
 to_char(v.user_registration_date, 'YYYY.MM.DD HH24:MI:SS'::text) as user_registration_date, 
v.user_login,
v.user_email, 
v.user_nickname,
uc.name_i18n as user_country_code,
v.user_city,
ut.md_code_i18n as user_type, 
null as unapproved_media
from bump.bump_user v 
left outer join bump.bump_md_collection ut on ut.id = v.user_type_mdci
left outer join bump.bump_md_country uc on uc.id = v.user_country_coi

where  v.deleted_10 = 1;

COMMENT ON VIEW bump.deleted_users_view IS 'Törölt felhasználók';

CREATE VIEW bump.pending_advertisers_view AS 

select 
v.id, 
 to_char(v.user_registration_date, 'YYYY.MM.DD HH24:MI:SS'::text) as user_registration_date, 
v.user_login,
v.user_email, 
v.user_nickname,
uc.name_i18n as user_country_code,
v.user_city,
ut.md_code_i18n as user_type, 
v.user_role,
0 as unapproved_media
from bump.bump_user v 
left outer join bump.bump_md_collection ut on ut.id = v.user_type_mdci
left outer join bump.bump_md_country uc on uc.id = v.user_country_coi

where
user_approved_10=0 
and v.user_role in ('BABE','AGENCY','CLUB','AGENCY_BABE','ADVERTISER');

CREATE VIEW bump.registered_advertisers_view AS 
select 
v.id, 
 to_char(v.user_registration_date, 'YYYY.MM.DD HH24:MI:SS'::text) as user_registration_date, 
v.user_login,
v.user_email, 
v.user_nickname,
uc.name_i18n as user_country_code,
v.user_city,
ut.md_code_i18n as user_type, 
null as unapproved_media
from bump.bump_user v 
left outer join bump.bump_md_collection ut on ut.id = v.user_type_mdci
left outer join bump.bump_md_country uc on uc.id = v.user_country_coi

where  v.user_role in ('BABE','AGENCY','CLUB','ADVERTISER') and
v.deleted_10 = 0;

COMMENT ON VIEW bump.registered_advertisers_view IS 'Regisztrált felhasználók';

CREATE VIEW bump.registered_users_view AS 
select 
v.id, 
 to_char(v.user_registration_date, 'YYYY.MM.DD HH24:MI:SS'::text) as user_registration_date, 
v.user_login,
v.user_email, 
v.user_nickname,
uc.name_i18n as user_country_code,
v.user_city,
ut.md_code_i18n as user_type,  
null as unapproved_media
from bump.bump_user v 
left outer join bump.bump_md_collection ut on ut.id = v.user_type_mdci
left outer join bump.bump_md_country uc on uc.id = v.user_country_coi

where  v.deleted_10 = 0;

COMMENT ON VIEW bump.registered_users_view IS 'Összes eddigi regisztrált felhasználó';]]></string>
